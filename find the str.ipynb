   "source": [
    "# Find the number of occurrences of 'H' in the given string without using the count method\n",
    "k = \"Hi,HellO,how are you\"\n",
    "a={}\n",
    "for i in k:\n",
    "    if i not in a:\n",
    "        a[i]=1\n",
    "    else:\n",
    "        a[i]=a[i]+1\n",
    "a[\"H\"]"
   ]
